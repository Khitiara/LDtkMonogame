{
  // See https://go.microsoft.com/fwlink/?LinkId=733558
  // for the documentation about the tasks.json format
  "version": "2.0.0",
  "tasks": [
    {
      "label": "docfx build",
      "command": "docfx",
      "type": "process",
      "args": ["${workspaceFolder}/docfx_project/docfx.json"],
      "problemMatcher": "$msCompile"
    },
    {
      "label": "docfx serve",
      "command": "docfx",
      "type": "process",
      "args": ["serve", "${workspaceFolder}/docs/"],
      "problemMatcher": "$msCompile"
    }
  ]
}
